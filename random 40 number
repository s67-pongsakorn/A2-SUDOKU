import tkinter as tk
import random

def fill_random_entries(entries):
    positions = random.sample(range(81), 40)
    for pos in positions:
        row, col = divmod(pos, 9)
        entries[row][col].insert(0, str(random.randint(1, 9)))

root = tk.Tk()
input1 = [[tk.Entry(root, width=5) for _ in range(9)] for _ in range(9)]

for row in range(9):
    for col in range(9):
        input1[row][col].grid(row=row, column=col)

fill_random_entries(input1)

root.mainloop()
